# Bài 1: Tính tổng từ 1 đến n
n = int(input())
s = 0
for i in range(1, n+1):
    s += i
print(s)

# Bài 2: Kiểm tra số nguyên tố
n = int(input())
check = True
if n < 2:
    check = False
for i in range(2, int(n**0.5)+1):
    if n % i == 0:
        check = False
        break
print("YES" if check else "NO")


# Bài 3: Đảo ngược chuỗi
s = input()
print(s[::-1])
